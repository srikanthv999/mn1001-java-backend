pipeline {
    agent any
    stages {
        stage('Build')
        {
            steps 
            {
                sh '''#!/bin/bash
                echo Build Stage
                '''
            }
        }
      stage('Execute')
        {
            steps 
            {
                sh '''#!/bin/bash
                echo Execute Stage
                '''
            }
        }
      stage('Test')
        {
            steps 
            {
                sh '''#!/bin/bash
                echo Test Stage
                '''
            }
        }
        
        }
    }
